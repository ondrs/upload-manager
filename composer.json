{
  "name": "ondrs/upload-manager",
  "type": "library",
  "description": "Upload manager for Nette framework",
  "keywords": ["nette", "upload", "file upload"],
  "homepage": "http://ondraplsek.cz",
  "license": ["MIT"],
  "authors": [
    {
      "name": "Ondřej Plšek",
      "homepage": "http://ondraplsek.cz",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1",
    "ext-gd": "*",
    "nette/bootstrap": "~3.0.0",
    "nette/di": "~3.0.0",
    "nette/finder": "~3.0.0",
    "nette/http": "~3.0.0",
    "nette/utils":"~3.0.0",
    "guzzlehttp/guzzle":"~6.5.5"
  },
  "suggest": {
    "ext-exif": "to read and keep images EXIF for fixing image orientation"
  },
  "require-dev": {
    "nette/tester": "~1.7.0",
    "mockery/mockery": "^0.9.4"
  },
  "autoload": {
    "classmap": ["src/"]
  }
}
